<% pets.each do |pet| %>
  <div id="pet-<%= pet.id%>">
    <ul>
      <li>Name:
        <%=pet.name %>
      </li>
      <li>Age:
        <%=pet.age %>
      </li>
      <li>Breed:
        <%=pet.breed %>
      </li>
      <li>Shelter:
        <%=pet.shelter_name %>
      </li>
    </ul>
  </div>
<% end %>
